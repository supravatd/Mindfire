<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document List</title>
</head>
<body>
    <div class="form-group">
        <label for="ddlDocumentType">Select Document Type:</label>
        <select id="ddlDocumentType" class="form-control"></select>
    </div>
    <div class="form-group">
        <label for="fileUpload">Upload Document:</label>
        <div class="input-group">
            <div class="custom-file">
                <input type="file" id="fileUpload" class="custom-file-input" />
            </div>
            <div class="input-group-append">
                <button type="button" id="btnUpload" class="btn btn-primary">Upload</button>
            </div>
        </div>
    </div>

    <br />
    <div class="col-md-6">
        <div>
            <table id="documentGrid" class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Document ID</th>
                        <th>Object ID</th>
                        <th>Object Type</th>
                        <th>Document Type</th>
                        <th>Document Original Name</th>
                        <th>Download File</th>
                        <th>Added On</th>
                    </tr>
                </thead>
                <tbody id="documentGridBody">
                </tbody>
            </table>
        </div>
    </div>
</body>
</html>

