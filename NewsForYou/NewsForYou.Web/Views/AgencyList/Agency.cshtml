@model List<NewsForYou.Model.Model.AgencyModel>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>My Page</title>
    <link href="@Url.Content("~/Content/AgencyList.css")" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="container">
        <div class="row">
            @foreach (var agency in Model)
            {
                <div class="col-md-4 mb-4">
                    <a href="@Url.Action("NewsFeed", "Feed", new { agencyId = agency.AgencyId })" class="agency-link">
                        <div class="agency-box">
                            <img src="@Url.Content("~/" + agency.AgencyLogoPath)" alt="@agency.AgencyName" class="img-fluid" />
                            <div class="agency-name">@agency.AgencyName</div>
                        </div>
                    </a>
                </div>
            }
        </div>
    </div>
</body>
</html>